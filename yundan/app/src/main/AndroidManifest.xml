<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zt.yundan">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.CaptureActivity"/>
        <activity android:name=".activity.QrcConsappContentActivity"/>
        <activity android:name=".activity.TextHintActivity"/>
        <activity android:name=".activity.TrafficActivity"/>
        <activity android:name=".activity.PersonalActivity"/>
        <activity android:name=".activity.StatisticActivity"/>
    </application>
</manifest>
